<launch>

	<node name="ow_chassis" pkg="rosserial_python" type="serial_node.py" output="screen">
		<param name="port" value="/dev/serial/by-id/usb-1a86_USB2.0-Serial-if00-port0"/>
		<param name="baud" value="57600"/>
	</node>


<!--
	<node name="ow_chassis" pkg="fchassis" type="ros_fchassis.py" output="screen">
	</node>
-->


<!-- sensors now formed from state of fchassis
	<include file="$(find ow_sensors)/launch/sensors.launch">
	</include>
-->

	<include file="$(find raspicam_nd)/launch/raspicam_nd.launch">
	</include>

	<arg name="master_html" default="radxa"/>
	<node pkg="multimaster" type="master_sync.py" name="master_html" output="screen"> 
		<param name="foreign_master" value="http://$(arg master_html):11311" />
		<rosparam command="load" file="mm2radxa.yaml" />
	</node>

	<arg name="master_op" default="opcplus"/>
	<node pkg="multimaster" type="master_sync.py" name="master_op" output="screen"> 
		<param name="foreign_master" value="http://$(arg master_op):11311" />
		<rosparam command="load" file="mm2opcplus.yaml" />
	</node>


</launch>
