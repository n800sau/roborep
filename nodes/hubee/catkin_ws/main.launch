	<include file="$(find raspicam_nd)/launch/raspicam_nd.launch">
	</include>
-->

<!-- ComplementaryFilter launch file -->
  #### Nodelet manager ######################################################

  <node pkg="nodelet" type="nodelet" name="imu_manager" 
    args="manager" output="screen" />

  #### Complementary filter

  <node pkg="imu_complementary_filter" type="complementary_filter_node"
      name="complementary_filter_gain_node" output="screen">
<remap from="imu/data_raw" to="fchassis/imu"/>
<remap from="imu/mag" to="fchassis/mf"/>
    <param name="do_bias_estimation" value="true"/>
    <param name="do_adaptive_gain" value="true"/>
    <param name="use_mag" value="true"/>
    <param name="gain_acc" value="0.01"/>
    <param name="gain_mag" value="0.01"/>
  </node>

<!-- sensors now formed from state of fchassis
	<include file="$(find ow_sensors)/launch/sensors.launch">
	</include>
-->

</launch>
