<launch>

	<node name="ow_chassis" pkg="rosserial_python" type="serial_node.py" output="screen">
		<param name="port" value="/dev/serial/by-id/usb-1a86_USB2.0-Serial-if00-port0"/>
		<param name="baud" value="57600"/>
	</node>

	<node name="master_sync_hubee" pkg="master_sync_fkie" type="master_sync">
	</node>

	<node name="master_discovery_hubee" pkg="master_discovery_fkie" type="zeroconf">
	</node>

<!--
	<node name="master_discovery_hubee" pkg="master_discovery_fkie" type="master_discovery">
	</node>
-->

<!--
	<node name="ow_chassis" pkg="fchassis" type="ros_fchassis.py" output="screen">
	</node>
-->

